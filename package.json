{
  "name": "local-web-server",
  "version": "1.2.6",
  "description": "A simple web-server for productive front-end development",
  "bin": {
    "ws": "./bin/cli.js"
  },
  "main": "lib/local-web-server.js",
  "license": "MIT",
  "keywords": [
    "dev",
    "server",
    "web",
    "tool",
    "front-end",
    "development",
    "cors",
    "mime",
    "rest",
    "mock",
    "api",
    "proxy"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "tape test/*/*.js",
    "docs": "jsdoc2md -t jsdoc2md/api.hbs -p list lib/*.js > doc/api.md; echo",
    "cover": "istanbul cover ./node_modules/.bin/tape test/*.js && cat coverage/lcov.info | coveralls && rm -rf coverage; echo"
  },
  "repository": "https://github.com/75lb/local-web-server",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "dependencies": {
    "ansi-escape-sequences": "^2.2.2",
    "array-back": "^1.0.3",
    "command-line-tool": "75lb/command-line-tool",
    "config-master": "^2.0.3",
    "koa": "^2.0.0",
    "local-web-server-default-stack": "github:local-web-server/default-stack",
    "reduce-flatten": "^1.0.1",
    "typical": "^2.4.2",
    "walk-back": "^2.0.1"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^1.3.6",
    "koa-cache-control": "^1.0.0",
    "koa-livereload": "~0.2.0",
    "node-fetch": "^1.5.3",
    "req-then": "~0.2.4",
    "tape": "^4.6.0"
  }
}
